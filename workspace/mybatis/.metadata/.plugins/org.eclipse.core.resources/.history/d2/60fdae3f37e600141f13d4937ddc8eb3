import com.school.bankjava.mybatis.FactoryManager;

public class BankJavaTest {
	
	@Test
	public void 마이바티스의SqlSessionFactory를가져온다_널이아니기를기대한다(){
		//given(초기화)
		FactoryManager factoryManager = new FactoryManager();
		FactoryManager factoryManager2 = new FactoryManager();
		SqlSessionFactory sqlSessionFactory = factoryManager.getSqlSessionFactory();
		//when(테스트할 대상 실행)
		//then(assert)
		assertNotNull(sqlSessionFactory);
	}

	
}
